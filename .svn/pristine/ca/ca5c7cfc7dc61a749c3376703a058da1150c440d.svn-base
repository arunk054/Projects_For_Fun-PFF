<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>loader</servlet-name>
		<servlet-class>arun.predicket.LoaderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loader</servlet-name>
		<url-pattern>/loader</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>getMatches</servlet-name>
		<servlet-class>arun.predicket.RetrieveMatches</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getMatches</servlet-name>
		<url-pattern>/getMatches</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>getSpecific</servlet-name>
		<servlet-class>arun.predicket.GetSpecificMatch</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getSpecific</servlet-name>
		<url-pattern>/getSpecific</url-pattern>
	</servlet-mapping>	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>arun.predicket.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>	

	<servlet>
		<servlet-name>addPredicket</servlet-name>
		<servlet-class>arun.predicket.AddPredicketServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addPredicket</servlet-name>
		<url-pattern>/addPredicket</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>getPreds</servlet-name>
		<servlet-class>arun.predicket.GetPredickets</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getPreds</servlet-name>
		<url-pattern>/getPreds</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>dp</servlet-name>
		<servlet-class>arun.predicket.DeletePred</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>dp</servlet-name>
		<url-pattern>/dp</url-pattern>
	</servlet-mapping>
	
	
	<!-- <security-constraint>
        <web-resource-collection>
            <url-pattern>/loader</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>admin</role-name>
        </auth-constraint>
    </security-constraint>-->
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
