<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>predicket</application>
	<version>1</version>
	
	 <static-files>
	 	<include path="/**.html" />
	 	<include path="/**.ico" />
        <include path="/js/**.js" />
        <include path="/css/**.css" />
        <include path="/css/bg-images/**.png" />
        <include path="/css/bg-images/**.gif" />
    </static-files>
    
	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>
	
</appengine-web-app>